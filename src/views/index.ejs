<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>NodeJS Basic</title>
    <link rel="stylesheet" href="/css/style.css">

</head>

<body>

    <div class="topnav">
        <a class="active" href="#home">Home</a>
        <a href="#news">News</a>
        <a href="#contact">Contact</a>
        <a href="#about">About</a>
    </div>

    <div class="container">
            <h2>List User</h2>
            <table>
                <tr>
                    <th>ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Points</th>
                </tr>
                <% for(let i=0; i < data.length; i++) { %>
                    <tr>
                      <td><%= data[i].id %></td>
                      <td><%= data[i].firstName %></td>
                      <td><%= data[i].lastName %></td>
                      <td>
                        <a href="detail/user/<%= data[i].id %>">Detail</a>
                        <a href="detail/user/<%= data[i].id %>">Edit</a>
                        <a href="detail/user/<%= data[i].id %>">Delete</a>
                      </td>
                    </tr>
                 <% } %>
            
            </table>
    </div>


</body>

</html>